{
  "name": "@telnyx/webrtc",
  "version": "2.0.1",
  "description": "Telnyx WebRTC Client",
  "keywords": [
    "telnyx",
    "webrtc",
    "sip",
    "json-rpc",
    "soft phone",
    "freeswitch",
    "voip"
  ],
  "main": "lib/bundle.js",
  "module": "lib/bundle.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",

  "release-it": {
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "loglevel": "^1.6.8",
    "react-native-incall-manager": "^3.2.7",
    "react-native-webrtc": "^1.75.3",
    "uuid": "^7.0.2"
  }

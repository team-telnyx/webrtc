{
  "git": {
    "tagName": "webrtc/v${version}"
  },
  "github": {
    "release": true,
    "releaseName": "webrtc@${version}"
  },
  "plugins": {
    "@release-it/bumper": {
      "in": "VERSION"
    },
    "@release-it/conventional-changelog": {
      "preset": "conventionalcommits",
      "tagPrefix": "webrtc/v",
      "path": "./src"
    }
  }
}

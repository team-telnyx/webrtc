{
  "name": "@telnyx/webrtc",
  "version": "2.2.1",
  "description": "Telnyx WebRTC Client",
  "keywords": [
    "telnyx",
    "webrtc",
    "sip",
    "json-rpc",
    "soft phone",
    "freeswitch",
    "voip"
  ],
  "main": "lib/bundle.js",
  "module": "lib/bundle.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "test": "jest",
    "docs": "typedoc src",
    "release": "release-it",
    "compile": "../../node_modules/.bin/tsc -w",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "engines": {
    "node": ">=11"
  },
  "author": "Telnyx <support@telnyx.com> (https://www.telnyx.com/)",
  "repository": {
    "type": "git",
    "url": "git://github.com/team-telnyx/webrtc.git"
  },
  "bugs:": "https://github.com/team-telnyx/webrtc/issues",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.6.8",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/uuid": "^7.0.0",
    "@types/webrtc": "0.0.26",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "events": "^3.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "md-gum-polyfill": "^1.0.0",
    "prettier": "^2.1.2",
    "release-it": "^14.0.2",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.6"
  },
  "publishConfig": {
    "access": "public",
    "@telnyx:registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "git": {
      "tagName": "@telnyx/webrtc/v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "lint-staged": {
    "*.js, *.jsx, *.ts, *.tsx": [
      "eslint --fix"
    ]
  }
}

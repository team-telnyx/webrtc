    "release-it": "^13.5.8",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/uuid": "^7.0.2",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
